      <providers>
        <add name="CustomMembershipProvider" type="WebPresentationLayer.Infrastructure.CustomMembershipProvider" connectionStringName="EntityModel" enablePasswordRetrieval="false" enablePasswordReset="true" requiresQuestionAndAnswer="false" requiresUniqueEmail="true" maxInvalidPasswordAttempts="5" minRequiredPasswordLength="3" minRequiredNonalphanumericCharacters="0" passwordAttemptWindow="10" applicationName="/" />
      </providers>
    </membership>
    <roleManager enabled="true" defaultProvider="CustomRoleProvider">
      <providers>
        <add name="CustomRoleProvider" type="WebPresentationLayer.Infrastructure.CustomRoleProvider" connectionStringName="EntityModel" applicationName="/" />
      </providers>
    </roleManager>